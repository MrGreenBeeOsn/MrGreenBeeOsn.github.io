{
  "name": "my-react-app",
  "private": true,
  "proxy": "http://localhost:3005",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://mrgreenbeeosn.github.io/MrGreenBeeOsn.github.io",
  "scripts": {
    "start": "react-scripts start",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build:db": "node generate-db.js",
    "db:generate": "node generate-db.js",
    "db:update-likes": "node update-likes-only.js",
    "db:update-views": "node update-views-only.js",
    "db:update-all": "npm run db:update-views && npm run db:update-likes",
    "db:server": "json-server --watch src/data/db.json --port 3005",
    "db": "npm run db:generate && npm run db:server",
    "db:add-views": "node update-views-only.js",
    "db:add-likes": "node update-likes-only.js",
    "db:setup": "npm run db:generate && npm run db:add-views && npm run db:add-likes"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "vite": "^7.1.6"
  }
}
